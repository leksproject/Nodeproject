{
  "name": "parsewebapi",
  "version": "1.0.0",
  "description": "`npm run build`",
  "main": "index.js",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "devDependencies": {},
  "scripts": {
    "test": "jest --no-cache --runInBand --silent",
    "build": "tsc",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "tests/.*\\.(ts|js)$",
    "automock": false,
    "modulePaths": [
      "<rootDir>/node_modules"
    ]
  },
  "directories": {
    "test": "tests"
  }
}
